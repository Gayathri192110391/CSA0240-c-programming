#include<stdio.h>

int main()
{
    int i, j, N ;



    printf("Enter number of rows: ");
    scanf("%d", &N);
if(N>0)
{
    for(i=1; i<=N; i++)
    {
     
        for(j=1; j<=N-i; j++)
        {
            printf(" ");
        }

        
        for(j=i*i; j>=1; j--)
        {
            printf("%d", j);
        }

        printf("\n");
    }
}
else
{
	printf("invalid");
}
}
