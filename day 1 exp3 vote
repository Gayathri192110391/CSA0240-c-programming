#include<stdio.h>

int main()
{
	int a ;
	
	
	printf("Enter the age of the person: ");
	scanf("%d",&a);

	
	if (a>=18)
	{
		printf("Eigibal for voting");
	}
	else if(a<0 && a==0)
	{
		printf("enter valid number");
	}
	else
	{
		printf("Not eligibal for voting\n %d  years to get eligible",18-a);
	}	

	return 0;
}

